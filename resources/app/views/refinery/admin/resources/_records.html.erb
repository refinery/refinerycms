<%= render 'refinery/admin/search_header', :url => refinery.admin_resources_path %>
<div class="pagination_container">
  <% if @resources.any? %>
    <%= render 'resources' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong><%= t('.no_files_yet') %></strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
