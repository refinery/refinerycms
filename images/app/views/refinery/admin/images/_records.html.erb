<%= render 'refinery/admin/search_header', :url => refinery.admin_images_path %>
<div class="pagination_container">
  <% if @images.any? %>
    <%= render 'images' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_images_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
