<div id='recent_inquiries'>
  <h2>Latest Inquiries</h2>
  <ul class='clickable'>
    <% @recent_inquiries.each do |inquiry| %>
      <li class='<%= cycle("on", "on-hover", :name => 'recent_inquiries_cycle') %>'>
        <%= link_to truncate("#{inquiry.name} - #{inquiry.message}", :length => 100, :preserve_html_tags => true).html_safe, admin_inquiry_url(inquiry) %>
      </li>
    <% end %>
  </ul>
</div>
